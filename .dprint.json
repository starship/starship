{
  "incremental": true,
  "typescript": {
    "indentWidth": 4
  },
  "json": {},
  "markdown": {
    "lineWidth": 100
  },
  "toml": {},
  "includes": [
    "docs/**/*.{ts,js,cjs,mjs}",
    "**/*.{md,toml,json}"
  ],
  "excludes": [
    "CHANGELOG.md",
    "docs/.vuepress/dist/**",
    "**/node_modules",
    "**/*-lock.json",
    ".github/*",
    "docs/ar-SA/**",
    "docs/ckb-IR/**",
    "docs/de-DE/**",
    "docs/es-ES/**",
    "docs/fr-FR/**",
    "docs/id-ID/**",
    "docs/it-IT/**",
    "docs/ja-JP/**",
    "docs/ko-KR/**",
    "docs/nl-NL/**",
    "docs/pl-PL/**",
    "docs/pt-BR/**",
    "docs/pt-PT/**",
    "docs/ru-RU/**",
    "docs/tr-TR/**",
    "docs/vi-VN/**",
    "docs/zh-CN/**",
    "docs/zh-TW/**",
    "target/"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.68.2.wasm",
    "https://plugins.dprint.dev/json-0.15.2.wasm",
    "https://plugins.dprint.dev/markdown-0.13.2.wasm",
    "https://plugins.dprint.dev/toml-0.5.4.wasm"
  ]
}
